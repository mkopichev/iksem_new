<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_protocols"
        android:name="com.iksem.ui.protocols.ProtocolsFragment"
        android:label="@string/title_protocols"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.iksem.ui.map.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_protocols" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.iksem.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_bluetooth"
        android:name="com.iksem.ui.bluetooth.BluetoothFragment"
        android:label="@string/title_bluetooth"
        tools:layout="@layout/fragment_bluetooth" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.iksem.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />

</navigation>